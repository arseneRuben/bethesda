                                        <br/>
                                        <br/>
                                        {{ std.birthplace }}
                                    </span>
                                    <br/>
                                </h3>

                                <h3>
                                    <span>
                                        Sexe: 
                                                                                                                                                                                                                                        
                                                                                                                                                                                                        
                                                                                                                                                                
                                                                                                                        
                                                                                
                                        {% if std.gender %}
                                            FEMININ
                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                        
                                                                                                                                                                            
                                                                                                                                
                                                                                    
                                        {% else %}
                                            MASCULIN 
                                                                                                                                                                                                                                                            
                                                                                                                                                                                                                        
                                                                                                                                                                            
                                                                                                                                
                                                                                    
                                        {% endif %}
                                    </span>
                                    <br/>
                                </h3>
                            </td>
                             <td style=" width: 30%;">
                                  {% if(fileExists[ std.id ])%}
                                              {% set imagePath = absolute_url(asset('assets/images/student/'~ std.imageName )) %}
