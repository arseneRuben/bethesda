                                    <br/>
                                </h3>
                                <br/>
                                <h3>
                                    <span>
                                        Classe/class :
                                                                                                                                                                                                                                    
                                                                                                                                                                                                    
                                                                                                                                                            
                                                                                                                    
                                                                            
                                    </span>
                                    <br/>
                                    <br/>
                                    <b>
                                        {{ room.name }}
                                    </b>
                                </h3>
                                <br/>
                                                                                                                                            
                                                                                                                                 
                            </td>
                        </tr>
                    </table>
                    <br/>
                    <table cellspacing="0" align="center" cellpadding="5" border="1" WIDTH="98%" style="line-height: {{ lineHeight }}em;">
                        <tr height="13px" WIDTH="15px" style="background-color:#EBECEC;" border="0.5">
