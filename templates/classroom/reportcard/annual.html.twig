        <title>
            {% if room.level.cycle.section.name == "Francophone" %}
                Bulletins annuel
                                                                                
                                                                                
                                                                                        
                                                                            
                                                            
                                            
                            
            {% else %}
               Annual report card
                                                                                
                                                                                
                                                                                        
                                                                            
                                                            
                                            
                            
            {% endif %}
            {{ room.name }}
        </title>
    </head>
    <body>

        {% for std in students %}
