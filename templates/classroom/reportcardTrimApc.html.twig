												{% else %}

											{% endif %}
										</i>
									</b>
								</td>
								<td border="0.5">
									<b>{{ totalCoefTrim  }}</b>
								</td>
								<td border="0.5">
									<b>
										<i>

											{{totalNCoefTrim}}
										</i>
									</b>
								</td>
								<td border="0.5">
									<b></b>
								</td>
