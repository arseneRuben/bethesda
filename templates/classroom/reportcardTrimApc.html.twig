								{# Total partiel des coefficients du module dans la sequence 1 #}
								{% set NCoefMod1 =0 %}
								{# Total partiel des Note * coefficient du module dans la séquence 1 #}
								{% set totalCoefModule2 =0 %}
								{# Total partiel des coefficients du module   dans la sequence 2 #}
								{% set NCoefMod2 =0 %}
								{# Total partiel des Note * coefficient du module dans la séquence 2 #}
								{% set NCoefModTrim =0 %}
								{# Total partiel des Note * coefficient du module dans le trimestre #}
								{% set totalCoefModTrim =0 %}
								{# Total partiel des coefficients du module   dans le trimestre #}
								{% for course in module.courses %}
									{% set break = false %}
									{% for mark1 in dataseq1 %}
										{% if std.matricule == mark1.matricule %}
											{% if module.name == mark1.module %}
												{% if mark1.wording == course.wording  and not break  %}
													<tr border="0.5" style="text-align:center;">
														<td border="0.5">
															{{course.wording |raw}}
														</td>
														<td border="0.5">
															{% if mark1.weight == 1 %}
																{{mark1.valeur}}
															{% else %}
																/
															{% endif %}
															{% set totalCoefModule1 = totalCoefModule1 + course.coefficient * mark1.weight %}
															{% set NCoefMod1 = NCoefMod1 +  mark1.valeur * course.coefficient * mark1.weight %}

														</td>
														{% for mark in dataseq2 %}
															{% if std.matricule == mark.matricule %}
																{% if module.name == mark.module %}
																	{% if mark.wording == course.wording  and not break  %}
																		<td border="0.5">
																			{% if mark.weight == 1 %}
																				{{mark.valeur}}
																				
																			{% else %}
																				/
																			{% endif %}
																			{% set totalCoefModule2 = totalCoefModule2 + course.coefficient * mark.weight %}
																				{% set NCoefMod2 = NCoefMod2 +  mark.valeur * course.coefficient * mark.weight %}

																		</td>
																		<td border="0.5" class="notetrim">
																			{% if ((mark1.weight == 1) or (mark.weight == 1)) %}
																				{{(((mark1.valeur * mark1.weight + mark.valeur * mark.weight)/(mark1.weight + mark.weight)))|round(2,'floor')}}

																			{% else %}
																				/
																			{% endif %}
																		</td>
																		<td border="0.5">{{course.coefficient * max(mark.weight, mark1.weight) }}</td>
																		<td border="0.5" cellspacing="1">
																			{% if (mark1.weight == 1) or (mark.weight == 1) %}
																				{{((mark1.valeur * mark1.weight + mark.valeur * mark.weight)/(mark1.weight + mark.weight) ) * course.coefficient |round(2,'floor')}}
																				{% set NCoefModTrim = NCoefModTrim + ((mark1.valeur * mark1.weight + mark.valeur * mark.weight)/(mark1.weight + mark.weight))  * course.coefficient * max(mark.weight, mark1.weight) %}
																				{% set totalCoefModTrim = totalCoefModTrim +   course.coefficient * max(mark.weight, mark1.weight) %}
																			{% else %}
																				/
																			{% endif %}
																		</td>
																		<td border="0.5"></td>
																	<td border="0.5">
																		{% if (mark.competence|lower  == "ras") %}
																		{% else %}
																			{{mark.competence|striptags|raw |slice(0,35)}}
																			{% endif %}
																		
																	</td>
																		<td border="0.5">{{mark.rank}}</td>
																		<td border="0.5">{{mark.teacher}}</td>
																		<td border="0.5"></td>

																		{% set break = true %}

																	{% endif %}
																{% endif %}
															{% endif %}
														{% endfor %}

													</tr>
													{% set break = true %}

												{% endif %}
											{% endif %}
										{% endif %}

									{% endfor %}

								{% endfor %}


