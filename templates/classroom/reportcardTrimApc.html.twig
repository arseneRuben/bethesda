																															{{ ((NCoefMod1 * totalCoefModule1 + NCoefMod2 * totalCoefModule2) / (totalCoefModule1 + totalCoefModule2))|round(2,'floor') }}
																														{% else %}
																															/
																														{% endif %}
																														#}																		
									</td>


									<td border="0.5">
										<b>
											{{totalCoefModTrim}}
										</b>
									</td>
									<td border="0.5">
										<b>
											{{NCoefModTrim}}
										</b>
									</td>

									<td border="0.5">
										<b></b>
									</td>
									<td border="0.5">
										<b></b>
									</td>
									<td border="0.5">
										<b></b>
									</td>
									<td border="0.5">
										<b></b>
									</td>
									<td border="0.5">
										<b></b>
									</td>
								</tr>
							{% endfor %}
