{% block javascripts %}
    {{ parent() }}
    <script src="https://d3js.org/d3-scale-chromatic.v1.min.js"></script>

    <script>
	// set the dimensions and margins of the graph
var margin = {top: 10, right: 30, bottom: 20, left: 30},
width = 1000 - margin.left - margin.right,
height = 400 - margin.top - margin.bottom;

// append the svg object to the body of the page
var svg = d3.select("#my_dataviz")
.append("svg")
.attr("width", width + margin.left + margin.right)
.attr("height", height + margin.top + margin.bottom)
.append("g")
.attr("transform",
	  "translate(" + margin.left + "," + margin.top + ")");

// Parse the Data
d3.csv("result.csv", function(data) {

// List of subgroups = header of the csv files = soil condition here
var subgroups = data.columns.slice(1)

// List of groups = species here = value of the first column called group -> I show them on the X axis
var groups = d3.map(data, function(d){return(d.group)}).keys()

// Add X axis
var x = d3.scaleBand()
  .domain(groups)
  .range([0, width])
  .padding([0.2])
svg.append("g")
.attr("transform", "translate(0," + height + ")")
.call(d3.axisBottom(x).tickSizeOuter(0));

// Add Y axis
var y = d3.scaleLinear()
.domain([0, 100])
.range([ height, 0 ]);
svg.append("g")
.call(d3.axisLeft(y));

// color palette = one color per subgroup
var color = d3.scaleOrdinal()
.domain(subgroups)
.range(['#377eb8','#ffffcc'])

//stack the data? --> stack per subgroup
var stackedData = d3.stack()
.keys(subgroups)
(data)

// Show the bars
svg.append("g")
.selectAll("g")
// Enter in the stack data = loop key per key = group per group
.data(stackedData)
.enter().append("g")
  .attr("fill", function(d) { return color(d.key); })
  .selectAll("rect")
  // enter a second time = loop subgroup per subgroup to add all rectangles
  .data(function(d) { return d; })
  .enter().append("rect")
	.attr("x", function(d) { return x(d.data.group); })
	.attr("y", function(d) { return y(d[1]); })
	.attr("height", function(d) { return y(d[0]) - y(d[1]); })
	.attr("width",x.bandwidth())
});

svg.append("text")
    .text("effectif")
    .attr("transform", "rotate(-90)")
	.attr("x",30)
	.attr("y",-20)
    .attr("font-family" , "sans-serif")
    .attr("font-size" , "14px")
    .attr("fill" , "black")
    .attr("text-anchor", "middle");

svg.append("circle").attr("cx",920).attr("cy",130).attr("r", 6).style("fill", "#377eb8")
svg.append("circle").attr("cx",920).attr("cy",160).attr("r", 6).style("fill", "#ffffcc")
svg.append("text").attr("x", 1100).attr("y", 130).text("%SUCCESS").style("font-size", "15px").attr("alignment-baseline","middle")
svg.append("text").attr("x", 1100).attr("y", 160).text("%ECHECS").style("font-size", "15px").attr("alignment-baseline","middle")
</script>

